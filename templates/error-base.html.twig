            src="https://code.jquery.com/jquery-3.4.1.js"
            integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
            crossorigin="anonymous">
    </script>
    <script src="{{ asset('js/burger-menu.js') }}"></script>
    <script src="{{ asset('js/login-submenu.js') }}"></script>
    <script src="{{ asset('js/flashbag.js') }}"></script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBxQPRo_7ra-gslPdThaY93luphLtmRFEk&callback=initMap"
            type="text/javascript"></script>
{% endblock %}
</body>
</html>